#include <iostream>
#include<string.h>
using namespace std;
int countcharacter(string str, char ch){
    int c=0;
    str=str.
    for(int i=0;i<str.length();i++){
        if(str[i]==ch){
            c++;
        }
    }
    return c;
}
int main(){
    string st;
    getline(cin,st);
    char cr;
    cout<<"enter char"<<endl;
    cin>>cr;
    cout<<countcharacter(st,cr);
}
