#include <iostream>
#include <string>
using namespace std;
int main(){
    string s;
    cout<<"Enter a string: ";
    cin>>s;
    int v=0;
    int c=0;
    for(int i=0;i<s.length();i++){
        if(s[i]=='a' || s[i]=='e' || s[i]=='i' || s[i]=='o' || s[i]=='u'){
            v++;
        }
        else c++;
    }
    cout<<"Number of vowels: "<<v<<endl;
    cout<<"Number of consonants: "<<c;
}
