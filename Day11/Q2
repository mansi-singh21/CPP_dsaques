#include <iostream>
using namespace std;
int main() {
   int n;
   cout<<"Enter size: "<<endl;
   cin>>n;
   int arr[n];
   for(int i=0;i<n;i++){
       cin>>arr[i];
   }
   cout<<"Enter target:"<<endl;
   int t;
   cin>>t;
   int f=-1;
   for(int i=0;i<n;i++){
       if(arr[i]==t){
           f=i+1;
           break;
       }
   }
   if(f==-1){
       cout<<f;
   }
   else{
       cout<<f;
   }
}
